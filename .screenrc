autodetach on
bell_msg "^G"
defkanji utf-8
defencoding utf-8
encoding utf-8 utf-8
cjkwidth on
defscrollback 10000
escape ^Jj
startup_message off
vbell off
caption always "%{= wb} %-w%{=bu dr}%n %t%{-}%+w %= %{=b wk} [%l] %{=b wb}%y/%m/%d(%D) %{=b wm}%c"

bind c screen
bind p prev
bind n next
bind l windowlist
bind q remove

termcapinfo xterm* ti@:te@

defbce "on"
term xterm-256color
termcapinfo xterm-256color 'is=rmJH?7h?1;4;6l'
